        "browserfs": "^1.4.3",
        "ethereum-types": "^3.0.0",
        "typedoc": "^0.16.9"
    },
    "devDependencies": {
        "@0x/ts-doc-gen": "^0.0.16",
        "typescript": "^3.5.3"
    }
